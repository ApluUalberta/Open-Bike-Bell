{
  "name": "open-bike-bell",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "android": "expo run:android",
    "android:release": "expo run:android --variant release",
    "ios": "expo run:ios",
    "lint": "eslint .",
    "start": "expo start --dev-client",
    "test": "jest",
    "dev": "expo start -c",
    "dev:stage": "expo start -c",
    "dev:prod": "expo start -c",
    "prepare": "husky install",
    "prebuild": "npx expo prebuild",
    "eas:localDev": "eas build --platform all --profile development",
    "eas:localIOSDev": "eas build --platform ios --profile development",
    "eas:localAndroidDev": "eas build --platform android --profile development",
    "eas:localIos": "eas build --platform ios --profile localInternal",
    "eas:localAndroid": "eas build --platform android --profile localInternal",
    "eas:localAll": "eas build --platform all --profile localInternal",
    "eas:stageIos": "eas build --platform ios --profile stageInternal",
    "eas:stageAndroid": "eas build --platform android --profile stageInternal",
    "eas:stageAll": "eas build --platform all --profile stageInternal",
    "eas:testProdIPA": "eas build --platform ios --profile production",
    "eas:testProdAAB": "eas build --platform android --profile production",
    "eas:testProdAll": "eas build --platform all --profile production",
    "tsc:check": "tsc --noEmit"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "subject-case": [
        2,
        "always",
        [
          "sentence-case"
        ]
      ]
    }
  },
  "dependencies": {
    "expo": "~50.0.14",
    "expo-av": "~13.10.5",
    "expo-dev-client": "~3.3.11",
    "expo-sensors": "^12.9.1",
    "expo-status-bar": "~1.11.1",
    "husky": "^9.0.11",
    "lpf": "^1.0.0",
    "react": "18.2.0",
    "react-native": "0.73.6",
    "react-native-easy-grid": "^0.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.45",
    "@types/react-native": "^0.73.0",
    "typescript": "^5.1.3"
  }
}
